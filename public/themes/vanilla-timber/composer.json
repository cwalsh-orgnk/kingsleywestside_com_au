{
  "name": "organikweb/vanilla-timber",
  "description": "Vanilla Timeber is a WordPress theme for component-based development using Timber and Advanced Custom Fields based on FlyntWP.",
  "type": "wordpress-theme",
  "license": "MIT",
  "prefer-stable": true,
  "authors": [
    {
          "name": "Organik Web",
          "email": "hello@organikweb.com.au"
    }
  ],
  "require": {
    "php": ">=7.4",
    "flyntwp/acf-field-group-composer": "^1.1",
    "erusev/parsedown": "^1.7",
    "timber/timber": "^2.1"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.8"
  },
  "extra": {
    "installer-paths": {
      "vendor/{$vendor}/{$name}/": [
        "type:wordpress-muplugin",
        "type:wordpress-plugin",
        "type:wordpress-theme"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "Flynt\\": "lib/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
