<flynt-component class="componentSpacing" name="NavigationFooter" data-theme="dark">
  <div class="container">
    <div class="footer-main">
      <div class="footer-left">
          <a class="logo" href="{{ site.link|e('esc_url') }}">
            <img height="91" width="299" class="logo" src="{{ theme_uri }}/assets/images/logo.png" alt="PCF Logo">
          </a>
          {% include 'Partials/_socialIcons.twig' with {'globalOptions': globalOptions} only %} 
      </div>
      
      <div class="footer-right">
          {% if menu %}
            <nav class="navigation" aria-label="{{ labels.ariaLabel }}">
              {% include 'Partials/_menu.twig' with {'items': menu.get_items, 'level': '0', 'maxLevel': maxLevel} only %}
            </nav>
          {% endif %}
      </div>
    </div>
  </div>
     <svg class="mark left" width="55" height="54" viewBox="0 0 55 54" fill="none" xmlns="http://www.w3.org/2000/svg">
      <rect width="8" height="54" fill="white"/>
      <rect x="-0.000976562" y="53.9985" width="8.00857" height="54.0021" transform="rotate(-89.9203 -0.000976562 53.9985)" fill="white"/>
      <rect x="35" y="37" width="8" height="17" fill="white"/>
      <rect y="20" width="9" height="17" transform="rotate(-90 0 20)" fill="white"/>
    </svg>
    <svg class="mark right" width="55" height="54" viewBox="0 0 55 54" fill="none" xmlns="http://www.w3.org/2000/svg">
      <rect width="8" height="54" fill="white"/>
      <rect x="-0.000976562" y="53.9985" width="8.00857" height="54.0021" transform="rotate(-89.9203 -0.000976562 53.9985)" fill="white"/>
      <rect x="35" y="37" width="8" height="17" fill="white"/>
      <rect y="20" width="9" height="17" transform="rotate(-90 0 20)" fill="white"/>
    </svg>
</flynt-component>
